{
  "name": "dainty",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(html|js|json|vue)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(html|js|json|vue)\""
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.13.1",
    "access-log": "^0.4.1",
    "core-js": "^3.8.3",
    "nuxt": "^2.14.12",
    "swiper": "^5.4.5",
    "vue-awesome-swiper": "^4.1.1",
    "vue-js-counter": "^1.0.3",
    "vue-tawk": "^1.0.1"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass-loader": "^10.1.1"
  }
}
